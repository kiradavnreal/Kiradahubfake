-- Ki·ªÉm tra executor v√† kick
local function checkExecutor()
    local executor = identifyexecutor and identifyexecutor() or "Unknown"
    if executor:lower():find("codex") or executor:lower():find("fluxus") then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "C·∫£nh B√°o",
            Text = "ƒê·ª´ng s√†i n√≥ kh√¥ng an to√†n",
            Duration = 5
        })
        wait(5)
        game:Shutdown()
    elseif executor:lower():find("arceus") then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "C·∫£nh B√°o",
            Text = "B·∫°n khinh th∆∞·ªùng ch√∫ng t√¥i?",
            Duration = 5
        })
        wait(5)
        game:Shutdown()
    end
end
checkExecutor()

