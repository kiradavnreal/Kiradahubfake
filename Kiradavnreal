repeat task.wait() until game:IsLoaded() and game:GetService("Players") and game.Players.LocalPlayer and game.Players.LocalPlayer:FindFirstChild("PlayerGui") 

-- Thêm Anti-AFK
local AntiAFK = coroutine.wrap(function()
    while true do
        local success, err = pcall(function()
            local VirtualUser = game:GetService("VirtualUser")
            VirtualUser:CaptureController()
            VirtualUser:SetKeyDown("w")
            wait(0.1)
            VirtualUser:SetKeyUp("w")
            print("Anti-AFK: Đã mô phỏng hoạt động")
        end)
        if not success then
            warn("Lỗi Anti-AFK: " .. tostring(err))
        end
        wait(60)
    end
end)
AntiAFK()

-- Thông báo Anti-AFK được kích hoạt
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Anti-AFK",
    Text = "Chế độ Anti-AFK đã được kích hoạt!",
    Duration = 5
})
