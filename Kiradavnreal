------ Tab: Blox Fruit
local Tab1o = MakeTab({Name = "Blox Fruit"})

AddButton(Tab1o, {
    Name = "Redz Hub (Updated 2025)",
    Callback = function()
        local success, err = pcall(function()
            local Settings = {
                JoinTeam = "Pirates",  -- Pirates/Marines
                Translator = true  -- true/false
            }
            local scriptContent = game:HttpGet("https://pastefy.app/XMcjaaIS/raw")
            print("Redz Hub: Đã tải script, kích thước: " .. #scriptContent .. " bytes")
            loadstring(scriptContent)(Settings)
        end)
        if not success then
            warn("Lỗi load Redz Hub: " .. tostring(err))
        end
    end
})

