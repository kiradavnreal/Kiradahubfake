-- Intro animation
local function _iA()
    local sG = Instance.new("ScreenGui", _g)
    sG.Name = "IntroGui"
    sG.IgnoreGuiInset = true

    local f = Instance.new("Frame", sG)
    f.Size = UDim2.new(0, 300, 0, 150)
    f.Position = UDim2.new(0.5, -150, 0.5, -75)
    f.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    f.BackgroundTransparency = 0.5
    f.BorderSizePixel = 0

    local tL = Instance.new("TextLabel", f)
    tL.Size = UDim2.new(1, 0, 0.6, 0)
    tL.BackgroundTransparency = 1
    tL.Text = "Kirada VN Hack\nT√°c gi·∫£: Kirada & Habato\nNg∆∞·ªùi l√†m & vi·∫øt: Kirada & Hi·∫øu TV"
    tL.TextColor3 = Color3.fromRGB(255, 255, 255)
    tL.TextScaled = true

    local iL = Instance.new("ImageLabel", f)
    iL.Size = UDim2.new(0, 100, 0, 100)
    iL.Position = UDim2.new(0.5, -50, 0.6, 0)
    iL.BackgroundTransparency = 1
    iL.Image = "rbxassetid://75676578090181"
    iL.ImageTransparency = 1

    pcall(function()
        _c:PreloadAsync({"rbxassetid://75676578090181"})
    end)

    local tI = TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.In)
    local fIT = _t:Create(tL, tI, {TextTransparency = 0})
    local fIF = _t:Create(f, tI, {BackgroundTransparency = 0.2})
    local fII = _t:Create(iL, tI, {ImageTransparency = 0})

    local fOT = _t:Create(tL, tI, {TextTransparency = 1})
    local fOF = _t:Create(f, tI, {BackgroundTransparency = 1})
    local fOI = _t:Create(iL, tI, {ImageTransparency = 1})

    fIT:Play()
    fIF:Play()
    fII:Play()
    fIT.Completed:Wait()

    wait(5)

    fOT:Play()
    fOF:Play()
    fOI:Play()
    fOT.Completed:Wait()

    sG:Destroy()
end
_iA()

