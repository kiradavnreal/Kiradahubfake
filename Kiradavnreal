-- H√†m th√™m n√∫t sao ch√©p link/key ho·∫∑c th·ª±c hi·ªán h√†nh ƒë·ªông
local function addButton(tab, name, callbackOrUrl)
    AddButton(tab, {
        Name = name,
        Callback = type(callbackOrUrl) == "function" and callbackOrUrl or function()
            local success, err = pcall(function()
                setclipboard(callbackOrUrl)
            end)
            StarterGui:SetCore("SendNotification", success and {
                Title = "Th√¥ng B√°o",
                Text = "ƒê√£ sao ch√©p link " .. name .. "!",
                Duration = 10
            } or {
                Title = "L·ªói",
                Text = "Kh√¥ng th·ªÉ sao ch√©p link " .. name .. ": " .. tostring(err),
                Duration = 5
            })
        end
    })
end

-- H√†m th√™m n√∫t ch·∫°y script
local function addScriptButton(tab, name, url)
    AddButton(tab, {
        Name = name,
        Callback = function()
            local success, err = pcall(function()
                loadstring(game:HttpGet(url))()
            end)
            StarterGui:SetCore("SendNotification", success and {
                Title = "Th√¥ng B√°o",
                Text = "ƒê√£ ch·∫°y script " .. name .. "!",
                Duration = 5
            } or {
                Title = "L·ªói",
                Text = "Kh√¥ng th·ªÉ ch·∫°y script " .. name .. ": " .. tostring(err),
                Duration = 5
            })
        end
    })
end

-- H√†m t√¨m v√† chuy·ªÉn ƒë·∫øn server c√≥ √≠t ng∆∞·ªùi ch∆°i nh·∫•t (∆∞u ti√™n server kh√¥ng c√≥ ng∆∞·ªùi)
local function hopToLowPlayerServer()
    local success, result = pcall(function()
        local apiUrl = "https://games.roblox.com/v1/games/" .. gameId .. "/servers/Public?sortOrder=Asc&limit=100"
        local response = game:HttpGet(apiUrl)
        local data = HttpService:JSONDecode(response)

        local servers = data.data
        if not servers or #servers == 0 then
            StarterGui:SetCore("SendNotification", {
                Title = "L·ªói",
                Text = "Kh√¥ng t√¨m th·∫•y server n√†o!",
                Duration = 5
            })
            return
        end

        -- T√¨m server c√≥ √≠t ng∆∞·ªùi ch∆°i nh·∫•t, ∆∞u ti√™n server kh√¥ng c√≥ ng∆∞·ªùi (playing = 0)
        local lowestPlayerCount = math.huge
        local targetServer = nil

        for _, server in ipairs(servers) do
            if server.playing and server.playing <= lowestPlayerCount and server.id ~= game.JobId then
                lowestPlayerCount = server.playing
                targetServer = server
                if server.playing == 0 then
                    break -- ∆Øu ti√™n server kh√¥ng c√≥ ng∆∞·ªùi
                end
            end
        end

        if targetServer then
            StarterGui:SetCore("SendNotification", {
                Title = "Th√¥ng B√°o",
                Text = "ƒêang chuy·ªÉn ƒë·∫øn server v·ªõi " .. lowestPlayerCount .. " ng∆∞·ªùi ch∆°i...",
                Duration = 5
            })
            TeleportService:TeleportToPlaceInstance(gameId, targetServer.id, LocalPlayer)
        else
            StarterGui:SetCore("SendNotification", {
                Title = "L·ªói",
                Text = "Kh√¥ng t√¨m th·∫•y server ph√π h·ª£p ƒë·ªÉ chuy·ªÉn!",
                Duration = 5
            })
        end
    end)

    if not success then
        StarterGui:SetCore("SendNotification", {
            Title = "L·ªói",
            Text = "Kh√¥ng th·ªÉ t√¨m server: " .. tostring(result),
            Duration = 5
        })
    end
end

-- H√†m chuy·ªÉn ƒë·∫øn VIP Server Real (s·ª≠ d·ª•ng link VIP server cho Blox Fruits, cho 99 ƒê√™m s·∫Ω th√¥ng b√°o kh√¥ng h·ªó tr·ª£)
local function hopToRealVipServer()
    local success, result = pcall(function()
        if gameId == 2753915549 then
            -- Link VIP server real cho Blox Fruits (c·∫ßn thay b·∫±ng code VIP th·ª±c t·∫ø)
            local vipLink = "https://www.roblox.com/games/2753915549?privateServerLinkCode=EXAMPLE_VIP_CODE_2025" -- Thay b·∫±ng code VIP real t·ª´ ngu·ªìn uy t√≠n
            StarterGui:SetCore("SendNotification", {
                Title = "Th√¥ng B√°o",
                Text = "ƒêang chuy·ªÉn ƒë·∫øn VIP Server Real...",
                Duration = 5
            })
            -- S·ª≠ d·ª•ng TeleportService v·ªõi private server link
            TeleportService:TeleportToPrivateServer(gameId, "EXAMPLE_VIP_CODE_2025", {LocalPlayer})
        else
            StarterGui:SetCore("SendNotification", {
                Title = "L·ªói",
                Text = "VIP Server Real ch·ªâ h·ªó tr·ª£ cho Blox Fruits. ƒê·ªëi v·ªõi 99 ƒê√™m, vui l√≤ng t√¨m link VIP th·ªß c√¥ng!",
                Duration = 5
            })
            return
        end
    end)

    if not success then
        StarterGui:SetCore("SendNotification", {
            Title = "L·ªói",
            Text = "Kh√¥ng th·ªÉ chuy·ªÉn ƒë·∫øn VIP Server: " .. tostring(result) .. "\nVui l√≤ng ki·ªÉm tra link VIP ho·∫∑c th·ª≠ l·∫°i!",
            Duration = 5
        })
    end
end

