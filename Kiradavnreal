-- Kiểm tra executor trước khi chạy script
local executor = identifyexecutor and identifyexecutor() or "Unknown"
if executor == "Fluxus" or executor == "Codex" then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Cảnh Báo",
        Text = "Fluxus và Codex không được phép sử dụng script này! Bạn đã bị kick và bị chặn IP trong 1 tiếng.",
        Duration = 10
    })
    wait(2) -- Đợi thông báo hiển thị
    game.Players.LocalPlayer:Kick("Fluxus và Codex bị cấm. Liên hệ Discord: https://discord.gg/kJ9ydA2PP4 để được hỗ trợ.")
    return -- Dừng script
end

loadstring(game:HttpGet(("https://raw.githubusercontent.com/daucobonhi/Ui-Redz-V2/refs/heads/main/UiREDzV2.lua")))()

-- Tạo UI tùy chỉnh cho intro với hiệu ứng làm mờ dần
local function createIntroAnimation()
    local TweenService = game:GetService("TweenService")
    local StarterGui = game:GetService("StarterGui")
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

    -- Tạo ScreenGui
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "IntroGui"
    screenGui.Parent = PlayerGui
    screenGui.IgnoreGuiInset = true

    -- Tạo Frame làm nền
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 300, 0, 150)
    frame.Position = UDim2.new(0.5, -150, 0.5, -75)
    frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    frame.BackgroundTransparency = 0.5
    frame.BorderSizePixel = 0
    frame.Parent = screenGui

    -- Tạo TextLabel cho thông báo
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.Text = "Kirada VN Hack\nTác giả: Kirada & Habato\nNgười làm & viết: Kirada & Hiếu TV"
    textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    textLabel.TextScaled = true
    textLabel.Font = Enum.Font.SourceSansBold
    textLabel.TextTransparency = 1 -- Bắt đầu với độ trong suốt
    textLabel.Parent = frame

    -- Tạo hiệu ứng làm mờ dần (fade-in)
    local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.In)
    local fadeIn = TweenService:Create(textLabel, tweenInfo, {TextTransparency = 0})
    local fadeInFrame = TweenService:Create(frame, tweenInfo, {BackgroundTransparency = 0.2})

    -- Tạo hiệu ứng làm mờ đi (fade-out)
    local fadeOut = TweenService:Create(textLabel, tweenInfo, {TextTransparency = 1})
    local fadeOutFrame = TweenService:Create(frame, tweenInfo, {BackgroundTransparency = 1})

    -- Chạy animation
    fadeIn:Play()
    fadeInFrame:Play()
    fadeIn.Completed:Wait()

    wait(5) -- Giữ thông báo trong 2 giây

    fadeOut:Play()
    fadeOutFrame:Play()
    fadeOut.Completed:Wait()

    -- Xóa GUI sau khi hoàn tất
    screenGui:Destroy()
end

-- Gọi hàm hiển thị animation
createIntroAnimation()

