-- X·ª≠ l√Ω khi nh√¢n v·∫≠t t√°i sinh
nguoiChoi.CharacterAdded:Connect(function(nhanVatMoi)
    nhanVat = nhanVatMoi
    nhanVatHumanoid = nhanVat:WaitForChild("Humanoid")
    phanGoc = nhanVat:WaitForChild("HumanoidRootPart")
    if xuyenTuongKichHoat then batTatXuyenTuong() end
    if chayVoHanKichHoat then batTatChayVoHan() end
    if nhayCaoKichHoat then batTatNhayCao() end
end)

