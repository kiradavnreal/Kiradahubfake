-- H√†m hi·ªÉn th·ªã th√¥ng b√°o m·ªôt l·∫ßn
local function hienThongBao(vanBan)
    local giaoDienManHinh = Instance.new("ScreenGui")
    giaoDienManHinh.Parent = nguoiChoi:WaitForChild("PlayerGui")
    giaoDienManHinh.Name = "ThongBaoKirada"
    giaoDienManHinh.ResetOnSpawn = false

    local khung = Instance.new("Frame")
    khung.Size = UDim2.new(0, 300, 0, 60)
    khung.Position = UDim2.new(0.5, -150, 0, 20)
    khung.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    khung.BorderSizePixel = 0
    khung.Parent = giaoDienManHinh
    local goc = Instance.new("UICorner")
    goc.CornerRadius = UDim.new(0, 10)
    goc.Parent = khung

    local nhan = Instance.new("TextLabel")
    nhan.Size = UDim2.new(1, 0, 1, 0)
    nhan.BackgroundTransparency = 1
    nhan.Text = vanBan
    nhan.TextColor3 = Color3.new(1, 1, 1)
    nhan.TextScaled = true
    nhan.Font = Enum.Font.GothamBold
    nhan.Parent = khung

    -- Hi·ªáu ·ª©ng xu·∫•t hi·ªán
    khung.Size = UDim2.new(0, 0, 0, 0)
    DichVuChuy·ªÉnƒê·ªông:Create(khung, TweenInfo.new(0.3, Enum.EasingStyle.Back), {Size = UDim2.new(0, 300, 0, 60)}):Play()
    
    -- Hi·ªáu ·ª©ng m·ªù d·∫ßn sau 3 gi√¢y
    wait(3)
    DichVuChuy·ªÉnƒê·ªông:Create(khung, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
    DichVuChuy·ªÉnƒê·ªông:Create(nhan, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
    wait(0.5)
    giaoDienManHinh:Destroy()
end

-- Hi·ªÉn th·ªã th√¥ng b√°o khi t·∫£i
hienThongBao(ngonNgu.vi.thongBao)

-- H√†m b·∫≠t/t·∫Øt xuy√™n t∆∞·ªùng
local function batTatXuyenTuong()
    xuyenTuongKichHoat = not xuyenTuongKichHoat
    if ketNoiXuyenTuong then ketNoiXuyenTuong:Disconnect() end
    
    if xuyenTuongKichHoat then
        ketNoiXuyenTuong = DichVuChay.Stepped:Connect(function()
            for _, phan in pairs(nhanVat:GetDescendants()) do
                if phan:IsA("BasePart") and phan.CanCollide then
                    phan.CanCollide = false
                end
            end
        end)
    end
end

-- H√†m b·∫≠t/t·∫Øt ch·∫°y v√¥ h·∫°n
local function batTatChayVoHan()
    chayVoHanKichHoat = not chayVoHanKichHoat
    if ketNoiChay then ketNoiChay:Disconnect() end
    
    if chayVoHanKichHoat then
        ketNoiChay = DichVuChay.Heartbeat:Connect(function()
            if nhanVatHumanoid.MoveDirection.Magnitude > 0 then
                phanGoc.Velocity = nhanVatHumanoid.MoveDirection * tocDoChay
            end
        end)
    end
end

-- H√†m b·∫≠t/t·∫Øt nh·∫£y cao
local function batTatNhayCao()
    nhayCaoKichHoat = not nhayCaoKichHoat
    nhanVatHumanoid.JumpPower = nhayCaoKichHoat and doCaoNhay or 50
end

-- H√†m c·∫≠p nh·∫≠t ng√¥n ng·ªØ giao di·ªán
local function capNhatNgonNgu()
    local ngonNguHienTai = tiengVietKichHoat and ngonNgu.vi or ngonNgu.en
    tieuDe.Text = ngonNguHienTai.tieuDe
    nhanTocDo.Text = ngonNguHienTai.nhanTocDo
    oNhapTocDo.PlaceholderText = ngonNguHienTai.goiYTocDo
    nutChay.Text = chayVoHanKichHoat and ngonNguHienTai.nutChayBat or ngonNguHienTai.nutChay
    nhanNhay.Text = ngonNguHienTai.nhanNhay
    oNhapNhay.PlaceholderText = ngonNguHienTai.goiYNhay
    nutNhay.Text = nhayCaoKichHoat and ngonNguHienTai.nutNhayBat or ngonNguHienTai.nutNhay
    nutXuyenTuong.Text = xuyenTuongKichHoat and ngonNguHienTai.nutXuyenTuongBat or ngonNguHienTai.nutXuyenTuong
    nutChuyenNgonNgu.Text = tiengVietKichHoat and ngonNguHienTai.nutChuyenNgonNguBat or ngonNguHienTai.nutChuyenNgonNgu
end

