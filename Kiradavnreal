-- Ch·ª©c nƒÉng k√©o th·∫£ menu
local dangKeo = false
local viTriBatDau
local viTriKhungBanDau

khungChinh.InputBegan:Connect(function(nhapLieu)
    if nhapLieu.UserInputType == Enum.UserInputType.MouseButton1 then
        dangKeo = true
        viTriBatDau = nhapLieu.Position
        viTriKhungBanDau = khungChinh.Position
    end
end)

khungChinh.InputChanged:Connect(function(nhapLieu)
    if dangKeo and nhapLieu.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = nhapLieu.Position - viTriBatDau
        khungChinh.Position = UDim2.new(
            viTriKhungBanDau.X.Scale,
            viTriKhungBanDau.X.Offset + delta.X,
            viTriKhungBanDau.Y.Scale,
            viTriKhungBanDau.Y.Offset + delta.Y
        )
    end
end)

khungChinh.InputEnded:Connect(function(nhapLieu)
    if nhapLieu.UserInputType == Enum.UserInputType.MouseButton1 then
        dangKeo = false
    end
end)

-- Hi·ªáu ·ª©ng xu·∫•t hi·ªán khung ch√≠nh
khungChinh.Size = UDim2.new(0, 0, 0, 0)
