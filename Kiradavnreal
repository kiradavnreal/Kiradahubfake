------ Tab: Discord
local Tab4o = MakeTab({Name = "Discord"})

AddButton(Tab4o, {
    Name = "Copy Discord Link",
    Callback = function()
        local success, err = pcall(function()
            setclipboard("https://discord.gg/kJ9ydA2PP4")
            print("Đã sao chép link Discord: https://discord.gg/kJ9ydA2PP4")
        end)
        if not success then
            warn("Lỗi sao chép link Discord: " .. tostring(err))
        else
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Discord",
                Text = "Đã sao chép link Discord vào clipboard!",
                Duration = 5
            })
        end
    end
})

