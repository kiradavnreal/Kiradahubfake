-- Script: KiraDaHub Beta Menu (Anime Style)
-- T√°c gi·∫£: Grok (t·∫°o theo y√™u c·∫ßu)
-- Ng√†y: 19/10/2025

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

-- T·∫°o GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "KiraDa hub beta"
ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

-- Frame ch√≠nh (Menu)
local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 250, 0, 350)
MainFrame.Position = UDim2.new(0.5, -125, 0.5, -175)
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
MainFrame.BorderSizePixel = 0
MainFrame.Visible = false
MainFrame.Parent = ScreenGui

-- G√≥c bo tr√≤n
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = MainFrame

-- Ti√™u ƒë·ªÅ (Anime Style)
local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 50)
Title.BackgroundTransparency = 1
Title.Text = "KiraDaHub Beta üò±"
Title.TextColor3 = Color3.fromRGB(255, 105, 180) -- M√†u h·ªìng anime
Title.TextScaled = true
Title.Font = Enum.Font.Anime
Title.Parent = MainFrame

-- Bi·∫øn tr·∫°ng th√°i
local isMenuOpen = false
local isSpeedEnabled = false
local isJumpEnabled = false
local isFlyEnabled = false

-- H√†m t·∫°o √¥ nh·∫≠p li·ªáu nh·ªè g·ªçn
local function CreateInputBox(name, placeholder, positionY)
    local InputBox = Instance.new("TextBox")
    InputBox.Size = UDim2.new(0.8, 0, 0, 30)
    InputBox.Position = UDim2.new(0.1, 0, 0, positionY)
    InputBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    InputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    InputBox.PlaceholderText = placeholder
    InputBox.Text = ""
    InputBox.TextScaled = true
    InputBox.Font = Enum.Font.Anime
    InputBox.Parent = MainFrame
    local UICornerInput = Instance.new("UICorner")
    UICornerInput.CornerRadius = UDim.new(0, 5)
    UICornerInput.Parent = InputBox
    return InputBox
end

-- H√†m t·∫°o n√∫t b·∫≠t/t·∫Øt nh·ªè g·ªçn
local function CreateToggleButton(name, positionY, callback)
    local ToggleButton = Instance.new("TextButton")
    ToggleButton.Size = UDim2.new(0.8, 0, 0, 30)
    ToggleButton.Position = UDim2.new(0.1, 0, 0, positionY)
    ToggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleButton.Text = name .. ": OFF"
    ToggleButton.TextScaled = true
    ToggleButton.Font = Enum.Font.Anime
    ToggleButton.Parent = MainFrame
    local UICornerButton = Instance.new("UICorner")
    UICornerButton.CornerRadius = UDim.new(0, 5)
    UICornerButton.Parent = ToggleButton

    local isToggled = false
    ToggleButton.MouseButton1Click:Connect(function()
        isToggled = not isToggled
        ToggleButton.Text = name .. (isToggled and ": ON" or ": OFF")
        ToggleButton.BackgroundColor3 = isToggled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(50, 50, 50)
        callback(isToggled)
    end)
    return ToggleButton
end

-- √î nh·∫≠p ID ng∆∞·ªùi ch∆°i
local PlayerIDInput = CreateInputBox("PlayerID", "Nh·∫≠p ID ho·∫∑c t√™n", 60)

-- N√∫t d·ªãch chuy·ªÉn
local TeleportButton = Instance.new("TextButton")
TeleportButton.Size = UDim2.new(0.8, 0, 0, 30)
TeleportButton.Position = UDim2.new(0.1, 0, 0, 100)
TeleportButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
TeleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TeleportButton.Text = "D·ªãch Chuy·ªÉn"
TeleportButton.TextScaled = true
TeleportButton.Font = Enum.Font.Anime
TeleportButton.Parent = MainFrame
local UICornerTeleport = Instance.new("UICorner")
UICornerTeleport.CornerRadius = UDim.new(0, 5)
UICornerTeleport.Parent = TeleportButton

-- H√†m t√¨m ng∆∞·ªùi ch∆°i theo ID ho·∫∑c t√™n
local function FindPlayer(input)
    local lowerInput = input:lower()
    for _, player in ipairs(Players:GetPlayers()) do
        if player.UserId == tonumber(input) or player.Name:lower():find(lowerInput) then
            return player
        end
    end
    return nil
end

-- X·ª≠ l√Ω d·ªãch chuy·ªÉn
TeleportButton.MouseButton1Click:Connect(function()
    local targetPlayer = FindPlayer(PlayerIDInput.Text)
    if targetPlayer and targetPlayer.Character and LocalPlayer.Character then
        LocalPlayer.Character:MoveTo(targetPlayer.Character.HumanoidRootPart.Position)
    end
end)

-- Hi·ªÉn th·ªã kho·∫£ng c√°ch c·∫ßu v·ªìng
local DistanceLabel = Instance.new("TextLabel")
DistanceLabel.Size = UDim2.new(0.8, 0, 0, 30)
DistanceLabel.Position = UDim2.new(0.1, 0, 0, 140)
DistanceLabel.BackgroundTransparency = 1
DistanceLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
DistanceLabel.Text = "Kho·∫£ng c√°ch: 0m"
DistanceLabel.TextScaled = true
DistanceLabel.Font = Enum.Font.Anime
DistanceLabel.Parent = MainFrame

-- C·∫≠p nh·∫≠t kho·∫£ng c√°ch
game:GetService("RunService").RenderStepped:Connect(function()
    local targetPlayer = FindPlayer(PlayerIDInput.Text)
    if targetPlayer and targetPlayer.Character and LocalPlayer.Character then
        local distance = (LocalPlayer.Character.HumanoidRootPart.Position - targetPlayer.Character.HumanoidRootPart.Position).Magnitude
        DistanceLabel.Text = string.format("Kho·∫£ng c√°ch: %.2fm", distance / 3) -- Quy ƒë·ªïi sang m√©t
        DistanceLabel.TextColor3 = Color3.fromRGB(
            math.sin(tick()) * 127 + 128,
            math.sin(tick() + 2) * 127 + 128,
            math.sin(tick() + 4) * 127 + 128
        ) -- Hi·ªáu ·ª©ng c·∫ßu v·ªìng
    else
        DistanceLabel.Text = "Kho·∫£ng c√°ch: N/A"
    end
end)

-- T·ªëc ƒë·ªô kh√¥ng gi·ªõi h·∫°n
local SpeedToggle = CreateToggleButton("T·ªëc ƒê·ªô", 180, function(state)
    isSpeedEnabled = state
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.WalkSpeed = state and 100 or 16
    end
end)

-- Nh·∫£y kh√¥ng gi·ªõi h·∫°n
local JumpToggle = CreateToggleButton("Nh·∫£y", 220, function(state)
    isJumpEnabled = state
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.JumpPower = state and 100 or 50
    end
end)

-- Bay kh√¥ng gi·ªõi h·∫°n
local FlyToggle = CreateToggleButton("Bay", 260, function(state)
    isFlyEnabled = state
    if state then
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.Parent = LocalPlayer.Character.HumanoidRootPart
        bodyVelocity.Name = "FlyVelocity"

        UserInputService.InputBegan:Connect(function(input)
            if isFlyEnabled and input.KeyCode == Enum.KeyCode.Space then
                bodyVelocity.Velocity = Vector3.new(0, 50, 0)
            end
        end)
        UserInputService.InputEnded:Connect(function(input)
            if isFlyEnabled and input.KeyCode == Enum.KeyCode.Space then
                bodyVelocity.Velocity = Vector3.new(0, 0, 0)
            end
        end)
    else
        if LocalPlayer.Character and LocalPlayer.Character.HumanoidRootPart:FindFirstChild("FlyVelocity") then
            LocalPlayer.Character.HumanoidRootPart.FlyVelocity:Destroy()
        end
    end
end)

-- B·∫≠t/t·∫Øt menu
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.Insert then -- Ph√≠m Insert ƒë·ªÉ b·∫≠t/t·∫Øt
        isMenuOpen = not isMenuOpen
        MainFrame.Visible = isMenuOpen
        Title.Text = isMenuOpen and "KiraDaHub Beta üò±" or "KiraDaHub Beta üò≠"
    end
end)

-- Th√¥ng b√°o t·∫£i script
print("KiraDaHub Beta ƒë√£ ƒë∆∞·ª£c t·∫£i! Nh·∫•n Insert ƒë·ªÉ m·ªü/t·∫Øt menu.")
