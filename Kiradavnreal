local success, err = pcall(function()
    -- Kiểm tra tải logo
    local ImageService = game:GetService("ContentProvider")
    ImageService:PreloadAsync({"rbxassetid://89326205091486"}) -- ID logo đã được thay
    print("Logo loaded successfully: rbxassetid://89326205091486")
end)
if not success then
    warn("Lỗi tải logo: " .. tostring(err) .. ". Kiểm tra ID ảnh hoặc executor.")
end

