-- H√†m th√™m n√∫t script
local function _aSB(t, n, u)
    AddButton(t, {
        Name = n,
        Callback = function()
            local s, e = pcall(function()
                loadstring(game:HttpGet(u))()
            end)
            if not s then
                _n:SetCore("SendNotification", {
                    Title = "L·ªói",
                    Text = "Kh√¥ng th·ªÉ ch·∫°y script " .. n .. ": " .. tostring(e),
                    Duration = 5
                })
            end
        end
    })
end

