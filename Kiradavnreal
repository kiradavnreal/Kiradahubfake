-- Ch·ªëng anti-cheat
local function _aC()
    local mt = getrawmetatable(game)
    local _ni = mt.__namecall
    setreadonly(mt, false)
    mt.__namecall = function(self, ...)
        local args = {...}
        local method = getnamecallmethod()
        if method == "FireServer" or method == "InvokeServer" then
            if tostring(self) == "AntiCheat" or tostring(self) == "AC" then
                return
            end
        end
        return _ni(self, ...)
    end
    setreadonly(mt, true)
end
pcall(_aC)

